   "source": [
    "def palindrome(s):\n",
    "    p=''\n",
    "    for i in s[::-1]:\n",
    "        p=str(p)+i\n",
    "    print(p)\n",
    "\n",
    "    if p==s:\n",
    "        print('Entered word is palindrom')\n",
    "    else:\n",
    "        print('not palindrom')\n"
   ]
